generator client {
  provider = "prisma-client-js"
  output   = "../app/generated/prisma"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model cereals {
  id                  Int    @id @default(autoincrement())
  name                String
  energy              Float
  protein             Float
  total_fat           Float
  total_carbohydrates Float
  fibra               Float
  calcium             Float
  fosforo             Float
  zinc                Float
  iron                Float
  sodium              Float
  potassium           Float
  image               String @default("none")
}

